@model IEnumerable<Employees.Models.Employee>

@{
    ViewData["Title"] = "Dashboard";
}

<p align="center">Top Employees</p>


<div id="containerIntro" align="center">

    <div class="gird grid-pad">
        <div class="col-sm-1-4">
            <div class="module employee">
                @foreach (var item in Model.Skip(2).Take(1))
                {

                    <h6><a class="at" asp-controller="Employees" asp-action="Details" asp-route-id="4">&nbsp;@Html.DisplayFor(modelItem => item.FirstName)&nbsp;&nbsp;@Html.DisplayFor(modelItem => item.LastName)&nbsp;</a></h6>






                }
            </div>
        </div>
    </div>

    <div class="gird grid-pad">
        <div class="col-sm-1-4">
            <div class="module employee">
                @foreach (var item in Model.Skip(3).Take(1))
                {
                    <h6><a class="at" asp-controller="Employees" asp-action="Details" asp-route-id="5">&nbsp;@Html.DisplayFor(modelItem => item.FirstName)&nbsp;&nbsp;@Html.DisplayFor(modelItem => item.LastName)&nbsp;</a></h6>

                }
            </div>
        </div>
    </div>

    <div class="gird grid-pad">
        <div class="col-sm-1-4">
            <div class="module employee">
                @foreach (var item in Model.Skip(4).Take(1))
                {
                    <h6><a class="at"  asp-controller="Employees" asp-action="Details" asp-route-id="6">&nbsp;@Html.DisplayFor(modelItem => item.FirstName)&nbsp;&nbsp;@Html.DisplayFor(modelItem => item.LastName)&nbsp;</a></h6>


                }
            </div>
        </div>
    </div>


    <div class="gird grid-pad">
        <div class="col-sm-1-4">
            <div class="module employee">
                @foreach (var item in Model.Skip(5).Take(1))
                {
                    <h6><a class="at"  asp-controller="Employees" asp-action="Details" asp-route-id="6">&nbsp;@Html.DisplayFor(modelItem => item.FirstName)&nbsp;&nbsp;@Html.DisplayFor(modelItem => item.LastName)&nbsp;</a></h6>


                }
            </div>
        </div>
    </div>
</div>
